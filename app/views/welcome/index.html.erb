<% if !session[:user_id] %>
  <%= button_to "Create New User", register_path, method: :get %>
  <%= button_to "Login", login_path, method: :get %>
<% else %>
  <%= button_to "Log Out", logout_path, method: :get %>
<section class='existing-users'>
    <p>Existing Users: </p>
    <% @users.each do |user| %>
        <li><%= user.email %></li>
    <% end %>
</section>
<% end %>